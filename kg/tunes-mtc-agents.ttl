        core:id                     "1463" ;
        core:involvesRole           <http://w3id.org/polifonia/resource/tunes/Role/Singer> ;
        core:name                   "Galiën, Tjeerd van der" ;
        core:placeOfBirthLatitude   "53.299431958" ;
        core:placeOfBirthLongitude  "5.97155066554" .

